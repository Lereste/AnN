{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "shop": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/shop",
      "sourceRoot": "projects/shop/src",
      "prefix": "app",
      "architect": {
        "serve": {
          "builder": "@angular-architects/native-federation:build",
          "options": {
            "target": "shop:serve-original:development",
            "rebuildDelay": 0,
            "dev": true,
            "port": 0
          }
        },
        "serve-original": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "shop:esbuild-original:production"
            },
            "development": {
              "buildTarget": "shop:esbuild-original:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "port": 1999
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "shop:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "projects/shop/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/shop/src/assets",
              {
                "glob": "**/*",
                "input": "projects/shop/src/assets"
              }
            ],
            "styles": ["@angular/material/prebuilt-themes/azure-blue.css", "projects/shop/src/styles.scss"],
            "scripts": []
          }
        },
        "esbuild": {
          "builder": "@angular-architects/native-federation:build",
          "options": {
            "target": "shop:esbuild-original:development",
            "rebuildDelay": 0,
            "dev": false
          },
          "configurations": {
            "production": {
              "target": "shop:esbuild-original:production",
              "dev": false
            },
            "development": {
              "target": "shop:esbuild-original:development",
              "dev": true
            }
          },
          "defaultConfiguration": "production"
        },
        "esbuild-original": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/shop",
            "index": "projects/shop/src/index.html",
            "browser": "projects/shop/src/main.ts",
            "polyfills": ["zone.js", "es-module-shims"],
            "tsConfig": "projects/shop/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "stylePreprocessorOptions": {
              "includePaths": ["node_modules", "projects/shop/src"]
            },
            "assets": [
              "projects/shop/src/assets",
              {
                "glob": "_redirects",
                "input": "projects/shop/src/assets",
                "output": "/"
              }
            ],
            "styles": ["@angular/material/prebuilt-themes/azure-blue.css", "projects/shop/src/styles.scss"],
            "scripts": [],
            "server": "projects/shop/src/main.server.ts",
            "outputMode": "server",
            "ssr": {
              "entry": "projects/shop/src/server.ts"
            },
            "prerender": false
          },
          "configurations": {
            "production": {
              "aot": true,
              "optimization": {
                "scripts": true,
                "styles": true,
                "fonts": false
              },
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "10mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "20kb",
                  "maximumError": "40kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        }
      }
    },
    "admin": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/admin",
      "sourceRoot": "projects/admin/src",
      "prefix": "app",
      "architect": {
        "serve": {
          "builder": "@angular-architects/native-federation:build",
          "options": {
            "target": "admin:serve-original:development",
            "rebuildDelay": 0,
            "dev": true,
            "port": 0
          }
        },
        "serve-original": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "admin:esbuild-original:production"
            },
            "development": {
              "buildTarget": "admin:esbuild-original:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "port": 2000
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "admin:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "projects/admin/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/admin/src/assets",
              {
                "glob": "**/*",
                "input": "projects/admin/src/assets"
              }
            ],
            "styles": ["@angular/material/prebuilt-themes/azure-blue.css", "projects/admin/src/styles.scss"],
            "scripts": []
          }
        },
        "esbuild": {
          "builder": "@angular-architects/native-federation:build",
          "options": {
            "target": "admin:esbuild-original:development",
            "rebuildDelay": 0,
            "dev": false
          },
          "configurations": {
            "production": {
              "target": "admin:esbuild-original:production",
              "dev": false
            },
            "development": {
              "target": "admin:esbuild-original:development",
              "dev": true
            }
          },
          "defaultConfiguration": "production"
        },
        "esbuild-original": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/admin",
            "index": "projects/admin/src/index.html",
            "browser": "projects/admin/src/main.ts",
            "polyfills": ["zone.js", "es-module-shims"],
            "tsConfig": "projects/admin/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "stylePreprocessorOptions": {
              "includePaths": ["node_modules", "projects/admin/src"]
            },
            "assets": [
              "projects/admin/src/assets",
              {
                "glob": "_redirects",
                "input": "projects/admin/src/assets",
                "output": "/"
              }
            ],
            "styles": ["@angular/material/prebuilt-themes/azure-blue.css", "projects/admin/src/styles.scss"],
            "scripts": [],
            "server": "projects/admin/src/main.server.ts",
            "outputMode": "server",
            "ssr": {
              "entry": "projects/admin/src/server.ts"
            },
            "prerender": false
          },
          "configurations": {
            "production": {
              "aot": true,
              "optimization": {
                "scripts": true,
                "styles": true,
                "fonts": false
              },
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "10mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "20kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        }
      }
    }
  },

  "cli": {
    "analytics": false
  }
}
