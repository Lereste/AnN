{
  "headers": [
    {
      "source": "/admin/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/admin/:match*",
      "destination": "https://admin-dienlanhhoaian.vercel.app/:match*"
    }
  ]
}
